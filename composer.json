{
    "name": "olobase",
    "type": "library",
    "description": "Olobase is a tool that allows to you quickly and easily develop modern open api based rest apis.",
    "license": "MIT",
    "keywords": [
        "olomadev",
        "mezzio",
        "laminas",
        "rest",
        "api",
        "oloma",
        "admin"
    ],
    "homepage": "https://oloma.dev",
    "autoload": {
