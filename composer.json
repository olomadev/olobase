        }
    },
    "autoload-dev": {
        "psr-4": { 
            "": ""
        }
    },
    "require": {
        "php": ">=8.0.0"
    },
    "extra": {
        "laminas": {
            "config-provider": "Olobase\\ConfigProvider"
        }
    }
}
